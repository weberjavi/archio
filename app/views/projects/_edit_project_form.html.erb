<%= form_for [current_user,@project], method: "patch" do |f| %>
<%= f.hidden_field :lat, value: @project.lat %>
<%= f.hidden_field :lng, value: @project.lng %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <br>
  <%= f.submit "Update" %>
<% end %>